  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            \"r\".\"review_id\",\n            \"r\".\"book_id\",\n            \"r\".\"user_id\",\n            \"r\".\"is_external\",\n            \"r\".\"source\",\n            \"r\".\"source_review_id\",\n            \"r\".\"user_id_src\",\n            \"r\".\"profile_name\",\n            \"r\".\"rating\",\n            \"r\".\"review_time\",\n            \"r\".\"summary\",\n            \"r\".\"content\",\n            \"r\".\"helpful_yes\" AS \"upvotes\",\n            \"r\".\"helpful_total\" - \"r\".\"helpful_yes\" AS \"downvotes\",\n            \"ra\".\"credibility_score\",\n            \"ra\".\"summary_ai\"\n        FROM \"reviews\" as \"r\"\n            LEFT JOIN \"review_ai\" AS \"ra\" ON \"r\".\"review_id\" = \"ra\".\"review_id\"\n        WHERE \"r\".\"review_id\" = $1;\n        ",
