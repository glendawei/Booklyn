  "query": "\n            SELECT \"ba\".\"book_id\", \"a\".\"author_id\", \"user_id\", \"name\", \"bio\", \"website\", \"created_at\"\n            FROM \"book_authors\" AS \"ba\"\n                JOIN \"authors\" AS \"a\" ON \"ba\".\"author_id\" = \"a\".\"author_id\"\n            WHERE \"ba\".\"book_id\" = ANY($1)\n            ORDER BY \"ba\".\"book_id\";\n            ",
