{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n            \"r\".\"review_id\",\n            \"r\".\"book_id\",\n            \"user_id\",\n            \"is_external\",\n            \"source\",\n            \"source_review_id\",\n            \"user_id_src\",\n            \"profile_name\",\n            \"rating\",\n            \"review_time\",\n            \"summary\",\n            \"content\",\n            \"helpful_yes\" AS \"upvotes\",\n            \"helpful_total\" - \"helpful_yes\" AS \"downvotes\",\n            \"ra\".\"credibility_score\",\n            \"ra\".\"summary_ai\"\n            FROM \"reviews\" as \"r\"\n                LEFT JOIN \"review_ai\" AS \"ra\" ON \"r\".\"review_id\" = \"ra\".\"review_id\"\n                JOIN \"books\" AS \"b\" ON \"r\".\"book_id\" = \"b\".\"book_id\"\n            WHERE \"r\".\"book_id\" = $1;\n            ",
